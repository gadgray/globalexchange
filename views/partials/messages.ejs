
<% if ( error_msg != ''){ %>
  <div class="alert alert-warning alert-dismissible fade show" role="alert">
    <p><%= error_msg%> </p>
    <button type="button" class="btn-close w-4" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  
  
  <%}%>

  <% if ( success_msg != ''){ %>
    <div class="alert alert-success alert-dismissible fade show" role="alert">
      <p><%= success_msg%></p>
      <button type="button" class="btn-close w-4" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    
    
    <%}%>
    <% if ( error != ''){ %>
      <div class="alert alert-success alert-dismissible fade show" role="alert">
        <p><%= error %></p>
        <button type="button" class="btn-close w-4" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
      
      
      <%}%>